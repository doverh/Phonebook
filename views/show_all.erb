<head>
<link rel="stylesheet" type="text/css" href="styles2.css">
</head>
<div class="login">

<center><h1>Phonebook</h1></center>
 <div class="half left cf">
   <table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Address</th>
	<th>City</th>
    <th>State</th>
    <th>Zip</th>
	<th>Phone</th>
    <th>email</th>
        
</tr>
 <% phonebook.each do |val| %>

<tr>
    <td><%= "#{val['first_name']}" %></td>
    <td><%= "#{val['last_name']}" %></td>
    <td><%= "#{val['address']}" %></td>
	<td><%= "#{val['city']}" %></td>
    <td><%= "#{val['state']}" %></td>
    <td><%= "#{val['zip']}" %></td>
	<td><%= "#{val['phone']}" %></td>
    <td><%= "#{val['email']}" %></td>
        
</tr>
<% end %>
</table>


</div>
<form class="cf" action = "/">
  <center><div> 
   <button type="submit" class="btn btn-primary btn-block btn-large">Insert new register</button>
  </div></center>
</form>